    "cz-conventional-changelog": "^2.1.0",
    "husky": "^0.14.3",
    "lint-staged": "^7.2.2",
    "node-dir": "^0.1.17",
    "node-fetch": "^2.1.1",
    "puppeteer": "^1.10.0",
    "react": "^16.4.2",
